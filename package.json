{
  "name": "trendypoll",
  "version": "1.0.0",
  "description": "This is the Trendypoll Backend",
  "main": "build/bin/www.js",
  "scripts": {
    "start": "yarn swagger_auto && node build/bin/www.js",
    "clean": "rm -rf build",
    "build": "yarn clean && yarn build:compile && yarn build:types",
    "build:compile": "npx babel src --extensions .ts --out-dir build",
    "build:types": "tsc",
    "dev": "yarn swagger_auto && concurrently yarn:dev:* --kill-others \"nodemon --exec node build/bin/www.js\"",
    "dev:compile": "npx babel src --extensions .ts --out-dir build --source-maps --watch",
    "dev:types": "tsc --watch",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install",
    "swagger_auto": "node build/swagger/swagger.js",
    "new_migrate": "npx sequelize-cli migration:generate",
    "run_migrate": "npx sequelize-cli db:migrate",
    "swagger": "tsoa spec",
    "test": "jest"
  },
